{
  "name": "vue-typescript-first",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "jest": "jest --coverage --logHeapUsage --maxWorkers=2 --no-cache --config jest.config.js",
    "jest:update": "npm run jest -- --updateSnapshot",
    "jest:updatesonar": "npm run jest -- --updateSnapshot && sonar-scanner"
  },
  "dependencies": {
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@vue/cli-plugin-e2e-cypress": "^3.0.1",
    "@vue/cli-plugin-typescript": "^3.0.1",
    "@vue/cli-plugin-unit-jest": "^3.0.1",
    "@vue/cli-service": "^5.0.8",
    "@vue/test-utils": "1.0.0-beta.29",
    "jest-junit": "^9.0.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-sonar-reporter": "^2.0.0",
    "jest-vue-preprocessor": "^1.7.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "ts-jest": "^23.0.0",
    "typescript": "^3.4.3",
    "vue-template-compiler": "^2.6.10"
  },
  "jestSonar": {
    "reportPath": "target/test-results/jest",
    "reportFile": "TESTS-results-sonar.xml"
  }
}